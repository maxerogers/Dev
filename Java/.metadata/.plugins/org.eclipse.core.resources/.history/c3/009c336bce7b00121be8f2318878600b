/**
 * This is my own binary tree
 * @author max
 *
 */
public class EZBinaryTree 
{
	private int[] array;
	public EZBinaryTree(int[] initSequenceArray) 
	{
		array = initSequenceArray;
	}
	public int findSmallestValue(int i, int j)
	{
		int result = array[array.length/2];
		if(result )
		return result;
	}
}
